---
import { Icon } from "astro-icon";
const { platform, username, set } = Astro.props;
---

<a
    class="flex my-3"
    href={`https://www.${platform}.com/${username}`}
    target="_blank"
    ><Icon
        class="w-6 mr-2 text-gray-900 hover:text-gray-500 transition-colors"
        name={`mdi:${platform}`}
    /></a
>
